<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            tools:context=".Fragment.DriverCabDetailsFragment">

            <include
                android:id="@+id/partincludedlayout"
                layout="@layout/layout_partneraddeddriver"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5" />

            <!--  Driver cab details layout-->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/partcab_details_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="20dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:background="@drawable/background_layout"
                android:visibility="gone"
                app:cardCornerRadius="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.5">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="8dp"
                        android:orientation="vertical"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginVertical="@dimen/_10sdp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="10dp"
                                android:layout_weight="1"
                                android:padding="8dp"
                                android:text="Cab Details"
                                android:textColor="#000000"
                                android:textSize="20sp"
                                android:textStyle="bold" />

                            <de.hdodenhof.circleimageview.CircleImageView
                                android:id="@+id/partupload_car"
                                android:layout_width="60dp"
                                android:layout_height="60dp"
                                android:layout_margin="10dp"
                                android:background="@drawable/circle_bg"
                                android:padding="2dp"
                                android:src="@drawable/cab"
                                android:visibility="gone" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_margin="10dp"
                            android:background="@drawable/input_boder_profile"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <!-- <EditText
                            android:id="@+id/partvechle_type"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@null"
                            android:hint="Vehicle Type"
                            android:padding="3dp"/>-->

                            <Spinner
                                android:id="@+id/partspinner_cabcategory"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_margin="10dp"
                            android:background="@drawable/input_boder_profile"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:visibility="visible">

                            <Spinner
                                android:id="@+id/partspinner_cabtype"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_margin="10dp"
                            android:background="@drawable/input_boder_profile"
                            android:gravity="center"
                            android:orientation="horizontal">


                            <Spinner
                                android:id="@+id/partvechle_model"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_margin="10dp"
                            android:background="@drawable/input_boder_profile"
                            android:gravity="center"
                            android:orientation="horizontal">


                            <Spinner
                                android:id="@+id/partmodelYear"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" />
                        </LinearLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="10dp">

                            <EditText
                                android:id="@+id/partvechle_no"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/input_boder_profile"
                                android:hint="Vehicle No."
                                android:imeOptions="actionNext"
                                android:inputType="text"
                                android:padding="15dp"
                                android:textSize="13sp" />

                        </com.google.android.material.textfield.TextInputLayout>


                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:visibility="gone">

                            <EditText
                                android:id="@+id/partregistration_no"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/input_boder_profile"
                                android:hint="Registration No."
                                android:padding="15dp"
                                android:textSize="13sp" />

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="10dp">

                            <EditText
                                android:id="@+id/partinsurance_no"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/input_boder_profile"
                                android:clickable="false"
                                android:cursorVisible="false"
                                android:drawableEnd="@drawable/ic_baseline_calendar_month_24"
                                android:focusable="false"
                                android:focusableInTouchMode="false"
                                android:hint="Insurance valid up to"
                                android:imeOptions="actionNext"
                                android:padding="10dp"
                                android:textSize="13sp" />

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="10dp">

                            <EditText
                                android:id="@+id/parttax_permit_no"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/input_boder_profile"
                                android:clickable="false"
                                android:cursorVisible="false"
                                android:drawableEnd="@drawable/ic_baseline_calendar_month_24"
                                android:focusable="false"
                                android:focusableInTouchMode="false"
                                android:hint="Local Permit valid up to"
                                android:imeOptions="actionNext"
                                android:padding="10dp"
                                android:textSize="13sp" />

                        </com.google.android.material.textfield.TextInputLayout>

                        <com.google.android.material.textfield.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="10dp"
                            android:layout_marginBottom="5dp">

                            <EditText
                                android:id="@+id/partnational_permit_date"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/input_boder_profile"
                                android:clickable="false"
                                android:cursorVisible="false"
                                android:drawableEnd="@drawable/ic_baseline_calendar_month_24"
                                android:focusable="false"
                                android:focusableInTouchMode="false"
                                android:hint="National Permit valid up to"
                                android:imeOptions="actionNext"
                                android:padding="10dp"
                                android:textSize="13sp" />


                        </com.google.android.material.textfield.TextInputLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:background="@drawable/input_boder_profile"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="5"
                                android:orientation="vertical"
                                android:padding="10dp">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Inbuilt GPS enabling tracking in real-time" />

                                <RadioGroup
                                    android:id="@+id/partradiogrp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <RadioButton
                                        android:id="@+id/partradioBTN_yes"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:text="yes" />

                                    <RadioButton
                                        android:id="@+id/partradioBTN_no"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="10dp"
                                        android:text="No" />
                                </RadioGroup>
                            </LinearLayout>

                            <ImageView
                                android:layout_width="0dp"
                                android:layout_height="100dp"
                                android:layout_weight="3"
                                android:src="@drawable/tracker" />
                        </LinearLayout>


                    </LinearLayout>
                </ScrollView>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <!-- working layout-->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/partwork"
                android:layout_width="match_parent"
                android:layout_height="514dp"
                android:layout_marginHorizontal="@dimen/_7ssp"
                android:layout_marginVertical="50dp"
                android:background="@drawable/background_layout"
                android:visibility="visible"
                app:cardCornerRadius="10dp"
                app:layout_constraintBottom_toTopOf="@+id/partbottombuttonbar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.5">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:padding="@dimen/_15sdp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginVertical="@dimen/_10sdp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Working Area"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_18sdp"
                                android:textStyle="bold" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginVertical="@dimen/_10sdp"
                            android:background="@color/light_grey"
                            android:padding="10dp">

                            <!--                 <EditText-->
                            <!--                android:layout_width="match_parent"-->
                            <!--                android:layout_height="match_parent"-->
                            <!--                android:background="@null"-->
                            <!--                android:hint="Outstation/Local/Both"-->
                            <!--                />-->
                            <Spinner
                                android:id="@+id/partworking_area_spinner"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Choose Working Area" />
                        </LinearLayout>


                        <!--            working state layout-->
                        <LinearLayout
                            android:id="@+id/partworking_state_layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:visibility="visible">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_10sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">


                                <Spinner
                                    android:id="@+id/partselect_state1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Choose Working State" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_10sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">


                                <Spinner
                                    android:id="@+id/partselect_state2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Choose Working State" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_10sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">

                                <Spinner
                                    android:id="@+id/partselect_state3"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Choose Working State" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_10sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">


                                <Spinner
                                    android:id="@+id/partselect_state4"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Choose Working State" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_10sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">


                                <Spinner
                                    android:id="@+id/partselect_state5"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Choose Working State" />
                            </LinearLayout>
                        </LinearLayout>


                        <!--            local working area layout-->
                        <LinearLayout
                            android:id="@+id/partworking_local_layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:visibility="gone">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_10sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">

                                <Spinner
                                    android:id="@+id/partselect_state_local"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Choose Working State" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_10sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">

                                <Spinner
                                    android:id="@+id/partselect_city"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Choose Working City" />
                            </LinearLayout>
                        </LinearLayout>


                    </LinearLayout>
                </ScrollView>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <!--  Image upload layout-->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/partupload_image"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginHorizontal="@dimen/_7ssp"
                android:background="@drawable/background_layout"
                android:visibility="gone"
                app:cardCornerRadius="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.5">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:padding="@dimen/_15sdp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Upload the Pictures"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_18sdp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:id="@+id/partImage_upload_layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:visibility="visible">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_5sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">

                                <TextView
                                    android:id="@+id/partselfiee"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableEnd="@drawable/upload"
                                    android:text="Selfie" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_5sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">

                                <TextView
                                    android:id="@+id/partdriving_licence"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableEnd="@drawable/upload"
                                    android:text="Driving Licence" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_5sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">


                                <TextView
                                    android:id="@+id/partcab_insurance"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableEnd="@drawable/upload"
                                    android:text="Cab Insurance" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_5sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">

                                <TextView
                                    android:id="@+id/partregistration_certificate"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableEnd="@drawable/upload"
                                    android:text="Registration Certificate" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_5sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">


                                <TextView
                                    android:id="@+id/partnational_permit_img"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableEnd="@drawable/upload"
                                    android:text="National Permit" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_5sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">


                                <TextView
                                    android:id="@+id/partlocal_permit_img"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableEnd="@drawable/upload"
                                    android:text="Local Permit" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_5sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">


                                <TextView
                                    android:id="@+id/parttaxi_front_pic_img"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableEnd="@drawable/upload"
                                    android:text="Taxi Front Pic \nwith number plate" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_5sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">


                                <TextView
                                    android:id="@+id/partaadhar_front_img"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableEnd="@drawable/upload"
                                    android:text="Aadhar Card Front" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_5sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">


                                <TextView
                                    android:id="@+id/partaadhar_back_img"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableEnd="@drawable/upload"
                                    android:text="Aadhar Card Back" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_5sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">


                                <TextView
                                    android:id="@+id/partpolice_certification_img"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableEnd="@drawable/upload"
                                    android:text="Police Certification" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/parttrackerGPS"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginVertical="@dimen/_5sdp"
                                android:background="@color/light_grey"
                                android:padding="10dp">


                                <TextView
                                    android:id="@+id/partgpscertificte"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableEnd="@drawable/upload"
                                    android:text="Inbuilt GPS Certificate" />
                            </LinearLayout>
                        </LinearLayout>


                    </LinearLayout>
                </ScrollView>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <ProgressBar
                android:id="@+id/partprogress"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <!-- <Button
                 android:id="@+id/partproceed"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_gravity="bottom"
